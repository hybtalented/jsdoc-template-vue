{
  "name": "jsdoc-template-vue",
  "version": "0.1.1",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build-client && npm run build-server",
    "build-client": "vue-cli-service build --mode production --target app src/entry-client.js",
    "build-server": "vue-cli-service build --mode production --target lib --no-clean --formats commonjs src/entry-server.js",
    "lint": "vue-cli-service lint",
    "doc": "jsdoc -c examples/jsdoc.json"
  },
  "files": [
      "template/",
      "src/",
      "layout.html",
      "template.js",
      "vue.config.js"
  ],
  "dependencies": {
    "vue": "^2.6.12",
    "vue-router": "^3.5.1",
    "vue-server-renderer": "^2.6.12",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.0.0",
    "code-prettify": "^0.1.0",
    "core-js": "^3.6.5",
    "eslint": "^6.0.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "jquery": "^3.6.0",
    "less-loader": "^4.1.0",
    "minimist": "^1.2.5",
    "popper.js": "^1.16.1",
    "prettier": "^1.19.1",
    "taffydb": "^2.6.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "peerDependencies": {
    "jsdoc": "^3.6.6"
  },
  "description": "## Project setup ``` npm install ```",
  "main": "publish.js",
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hybtalented/jsdoc-template-vue.git"
  },
  "keywords": [
    "jsdoc",
    "template",
    "vue",
    "typescript"
  ],
  "author": "hybtalented@163.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hybtalented/jsdoc-template-vue/issues"
  },
  "homepage": "https://github.com/hybtalented/jsdoc-template-vue#readme"
}
